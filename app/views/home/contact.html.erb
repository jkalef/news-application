<h1>Contact Us</h1>


<%= form_for @contact do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :subject %>
  <%= f.text_field :subject %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :message %>
  <%= f.text_area :message %>
  <br>
  <%= f.submit "Submit" %>
<% end %>